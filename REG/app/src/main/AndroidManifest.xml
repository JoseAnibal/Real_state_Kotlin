<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reg">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.REG">
        <activity android:name=".ChatPrivado.ChatPrivado"></activity>
        <activity android:name=".Actividades.NotLoggedUserVerPiso" />
        <activity android:name=".Actividades.RegistroActivity" />
        <activity
            android:name=".Actividades.Admin"
            android:label="@string/title_activity_admin"
            android:theme="@style/Theme.REG.NoActionBar" />
        <activity
            android:name=".Actividades.LoggedUser"
            android:label="@string/title_activity_logged_user"
            android:theme="@style/Theme.REG.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Actividades.MainActivity"
            android:label="@string/app_name" />
    </application>

</manifest>